def solution(a,b):
    lst = []
    if a > b:
        c = b
        d = a
        while (c <= d):
            lst.append(c)
            c += 1
        return(sum(lst))
    elif a == b:
        return(a)
    else:
        while (a <= b):
            lst.append(a)
            a += 1
        return(sum(lst)) 
