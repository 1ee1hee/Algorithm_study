def solution(array, commands):
    l = []
    s = []
    for a in range(len(commands)):
        for b in range(len(commands[a])):
            l.append(commands[a][b])
    i = l[:len(l):3]
    j = l[1:len(l):3]
    k = l[2:len(l):3]
    for c, d, e in zip(i, j, k):
        s.append(sorted(array[c-1:d])[e-1])
    return s
